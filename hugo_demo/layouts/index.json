{
    "items" : [    {{ $first := true }}    {{ range $.Site.RegularPages -}}    {{ if $first }}
    {
        
        "url" : "{{ .Permalink }}",
        "title" : "{{ .Title }}",
        "content": {{ .Content | plainify | jsonify }}
    }    {{ $first = false }}    {{ else }}
    ,{
        
        "url" : "{{ .Permalink }}",
        "title" : "{{ .Title }}",
        "content": {{ .Content | plainify | jsonify }}
    }    {{ end }}    {{ end -}}
    ]
}